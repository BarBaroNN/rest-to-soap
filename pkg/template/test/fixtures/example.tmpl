{
    "ISOCode": "{{.ISOCode}}",
    "name": "{{.Name}}",
    "capitalCity": "{{.CapitalCity}}",
    "languages": [
        {{range $index, $lang := .Languages}}
        {
            "name": "{{$lang.Name}}",
            "code": "{{$lang.Code}}",
            "isOfficial": {{$lang.IsOfficial}},
            "speakers": {{$lang.Speakers}}
        }{{if not $index}},{{end}}
        {{end}}
    ],
    "population": {{.Population}},
    "area": {{.Area}},
    "currency": {
        "code": "{{.Currency.Code}}",
        "name": "{{.Currency.Name}}",
        "symbol": "{{.Currency.Symbol}}",
        "exchangeRate": {{.Currency.ExchangeRate}},
        "isActive": {{.Currency.IsActive}}
    },
    "timezones": [
        {{range $index, $tz := .Timezones}}
        "{{$tz}}"{{if not $index}},{{end}}
        {{end}}
    ],
    "borders": [
        {{range $index, $border := .Borders}}
        "{{$border}}"{{if not $index}},{{end}}
        {{end}}
    ],
    "region": "{{.Region}}",
    "subregion": "{{.Subregion}}",
    "flag": "{{.Flag}}",
    "isIndependent": {{.IsIndependent}},
    "gdp": {{.GDP}},
    "gini": {{.Gini}},
    "hdi": {{.HDI}},
    "coordinates": {
        "latitude": {{.Coordinates.Latitude}},
        "longitude": {{.Coordinates.Longitude}},
        "isValid": {{.Coordinates.IsValid}}
    }
} 